  "search-result-layout.customQuery#payment": {
    "props": {
      "querySchema": {
        "maxItemsPerPage": 4,
        "orderByField": "OrderByTopSaleDESC"
      }
    },
    "blocks": ["search-result-layout.desktop#payment"]
  },
  "search-result-layout.desktop#payment": {
    "children": ["search-content#payment"]
  },
  "search-content#payment": {
    "blocks": ["gallery#payment", "not-found"]
  },
  "gallery#payment": {
    "props": {
      "maxItemsPerRow": {
        "desktop": 4,
        "tablet": 3
      }
    },
    "blocks": ["product-summary.shelf#payment"]
  },
  "product-summary.shelf#payment": {
    "children": ["flex-layout.row#payment"]
  },
  "flex-layout.row#payment": {
    "props": {
      "preserveLayoutOnMobile": true
    },
    "children": ["flex-layout.col#payment"]
  },
  "flex-layout.col#payment": {
    "props": {
      "verticalAlign": "middle"
    },
    "children": [
      "product-summary-image#payment",
      "product-summary-name#payment",
      "product-summary-description"
    ]
  },
  "product-summary-name#payment": {
    "props": {
      "blockClass": "payment"
    }
  },
  "product-summary-image#payment": {
    "props": {
