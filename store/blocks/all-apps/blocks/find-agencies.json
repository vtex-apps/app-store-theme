{
  "flex-layout.col#all-apps-find-agencies": {
    "props": {
      "blockClass": "allAppsFindAgencies",
      "width": "48%"
    },
    "children": [
      "rich-text#all-apps-find-agencies",
      "link#all-apps-find-agencies"
    ]
  },
  "rich-text#all-apps-find-agencies": {
